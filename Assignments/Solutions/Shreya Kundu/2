#include<bits/stdc++.h>
using namespace std;
int main(){
	for(int i=1;i<=5;i++){
		for(int j=5;j>=i;j--)
		cout<<"*";
		for(int k=1;k<=2*i-1;k++)
		cout<<" ";
		for(int l=5;l>=i;l--)
		cout<<"*";
		cout<<"\n";
	}
	for(int i=5;i>=1;i--){
		for(int j=5;j>=i;j--)
		cout<<"*";
		for(int k=2*i-1;k>=1;k--)
		cout<<" ";
		for(int l=5;l>=i;l--)
		cout<<"*";
		cout<<"\n";
	}
	return 0;
}
