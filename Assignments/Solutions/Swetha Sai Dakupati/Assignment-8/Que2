	SEARCHING        : https://practice.geeksforgeeks.org/problems/count-zeros-in-a-sorted-matrix/1
*/

 
class Solution{
public:
	int countZeros(vector<vector<int>>A)
	{
		//code here
		int cnt=0;
		int n=A.size();
		for(int i=0;i<n;i++){
		    cnt+=count(A[i].begin(),A[i].end(),0);
		}
		return cnt;
	}
};
