/*  
          GEEKSFORGEEKS QUESTIONS ON TOPICS COVERED TILL NOW 
									    (REVISION)

	STRINGS          : https://practice.geeksforgeeks.org/problems/remove-spaces0128/1

*/

class Solution
{
  public:
    string modify (string s)
    {
        //code here.
        int i,j=0,n;
        n=s.length();
        for(i=0;i<n;i++){
            if(s[i]!=' ') s[j++]=s[i]; 
        }
        s.resize(j);
        return s;
    }
};
