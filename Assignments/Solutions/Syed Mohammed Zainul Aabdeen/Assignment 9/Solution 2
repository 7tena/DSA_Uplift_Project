#include <iostream>
using namespace std;
int main() {
    int m,n,count=0;
    cin>>m>>n;
    int a[m][n];
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            cin>>a[i][j];
    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            if(a[i][j]<0)   count++;
    cout<<count;
    return 0;
}
